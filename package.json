{
  "name": "@soundworks/helpers",
  "version": "1.0.0-alpha.2",
  "description": "Set of generic helpers for soundworks applications",
  "authors": [
    "Benjamin Matuszewski"
  ],
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    "./launcher.js": {
      "module": "./src/browser/launcher.js",
      "node": "./src/node/launcher.js",
      "types": "./types/launcher.js"
    },
    "./polyfills.js": {
      "node": "./src/node/polyfills.js",
      "default": "./src/browser/polyfills.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collective-soundworks/soundworks-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/collective-soundworks/soundworks-helpers/issues"
  },
  "homepage": "https://github.com/collective-soundworks/soundworks-helpers#readme",
  "keywords": [
    "soundworks"
  ],
  "scripts": {
    "api": "jsdoc-to-readme --src src/browser/launcher.js src/node/launcher.js",
    "lint": "npx eslint src && npx eslint tests",
    "preversion": "npm run api && npm run toc && git commit --allow-empty -am 'doc: update readme'",
    "test": "node tests/init-pages-screenshots.js",
    "toc": "markdown-toc -i README.md  --maxdepth 3"
  },
  "dependencies": {
    "@ircam/simple-components": "^2.0.1",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "chalk": "^5.2.0",
    "core-js": "^3.22.3",
    "lit": "^2.2.2",
    "regenerator-runtime": "^0.13.7",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@ircam/eslint-config": "^1.1.0",
    "jsdoc-to-readme": "^1.1.0",
    "markdown-toc": "^1.2.0",
    "mkdirp": "^1.0.4",
    "puppeteer": "^19.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  }
}
